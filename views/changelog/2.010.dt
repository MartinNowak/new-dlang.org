        - bugItem(1700, "ICE attempting to modify member of const return struct");
        - bugItem(1707, "'==' in TemplateParameterList in IsExpression causes segfault");
        - bugItem(1711, "typeof with delegate literal not allowed as template parameter");
        - bugItem(1713, "foreach index with tuples and templates fails");
        - bugItem(1718, "obscure exit with error code 5");
        - bugItem(1719, "Compiler crash or unstable code generation with scoped interface instances");
        - bugItem(1720, "std.math.NotImplemented missing a space in message");
        - bugItem(1724, "Internal error: toir.c 177");
        - bugItem(1725, "std.stream.BufferedFile.create should use FileMode.OutNew");
        - bugItem(1757, "there is an fault in phobos windows api interface");
        - bugItem(1762, "Wrong name mangling for pointer args of free extern (C++) functions");
        - bugItem(1767, "rejects-valid, diagnostic");
